{
  "name": "ysera",
  "version": "0.2.3",
  "description": "Ysera-ified micro-core",
  "main": "build/lib/index.js",
  "scripts": {
    "test": "gulp test",
    "build": "gulp compile",
    "example": "gulp compile && node ./build/examples/app.js"
  },
  "author": "Dhi Aurrahman <dio@hooq.tv>",
  "license": "MIT",
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-async-to-generator",
      "syntax-async-functions",
      [
        "babel-plugin-transform-builtin-extend",
        {
          "globals": [
            "Error"
          ]
        }
      ]
    ]
  },
  "dependencies": {
    "babel-regenerator-runtime": "^6.5.0",
    "media-typer": "^0.3.0",
    "raw-body": "^2.1.6",
    "reflect-r": "^1.0.0",
    "source-map-support": "^0.4.0",
    "url-match": "^0.1.0"
  },
  "devDependencies": {
    "ava": "^0.14.0",
    "babel-plugin-syntax-async-functions": "^6.8.0",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-plugin-transform-builtin-extend": "^1.1.0",
    "babel-preset-es2015": "^6.6.0",
    "gulp": "^3.9.1",
    "gulp-ava": "^0.11.0",
    "gulp-babel": "^6.1.2",
    "gulp-cached": "^1.1.0",
    "gulp-concat-util": "^0.5.5",
    "gulp-sourcemaps": "^1.6.0",
    "supertest": "^1.2.0",
    "supertest-as-promised": "^3.1.0"
  },
  "directories": {
    "example": "examples",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/diorahman/ysera.git"
  },
  "keywords": [
    "micro-core",
    "micro",
    "async",
    "rest",
    "microservice",
    "microservices",
    "server"
  ],
  "bugs": {
    "url": "https://github.com/diorahman/ysera/issues"
  },
  "homepage": "https://github.com/diorahman/ysera#readme"
}
